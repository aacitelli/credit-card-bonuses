{
  "name": "@andenacitelli/credit-card-bonuses",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "sls offline start --stage offline",
    "deploy:prod": "sls deploy --stage prod",
    "lint": "pnpm lint:biome && pnpm lint:typescript",
    "lint:biome": "biome lint --apply .",
    "lint:openapi": "redocly lint --extends=minimal src/api.yaml",
    "lint:typescript": "tsc --noEmit",
    "format": "prettier --write .",
    "test": "jest",
    "generate": "redocly lint --extends=minimal src/api.yaml && openapi-zod-client ./src/api.yaml --output src/generated/api.client.ts --export-schemas",
    "prepare": "husky"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "helmet": "^7.1.0",
    "papaparse": "^5.4.1",
    "serverless-http": "^3.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.5.3",
    "@redocly/cli": "^1.10.3",
    "@swc/core": "^1.4.2",
    "@swc/jest": "^0.2.36",
    "@tsconfig/recommended": "^1.0.3",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.11.20",
    "@types/papaparse": "^5.3.14",
    "@types/supertest": "^6.0.2",
    "@zodios/core": "^10.9.6",
    "chalk": "^5.3.0",
    "depcheck": "^1.4.7",
    "esbuild": "~0.20.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "mkdirp": "^3.0.1",
    "openai": "^4.28.0",
    "openapi-zod-client": "^1.15.1",
    "puppeteer": "^21.11.0",
    "serverless": "^3.38.0",
    "serverless-esbuild": "^1.51.0",
    "serverless-offline": "^13.3.3",
    "supertest": "^6.3.4",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json,md}": [
      "prettier --write"
    ],
    "*.ts": [
      "biome lint --apply"
    ]
  }
}
