{
  "name": "@andenacitelli/credit-card-bonuses",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky",
    "format": "prettier . --write",
    "lint": "pnpm run /^lint:.*/",
    "lint:format": "prettier --check .",
    "lint:linter": "eslint .",
    "lint:renovate": "renovate-config-validator",
    "lint:typecheck": "tsc --noEmit",
    "lint:openapi": "redocly lint --extends=minimal src/api.yaml",
    "test": "vitest run",
    "bump": "ncu --upgrade --target semver && pnpm i",
    "generate:schemas": "openapi-zod-client ./src/api.yaml --output src/generated/api.client.ts --export-schemas",
    "generate:exports": "tsx ./bin/generate.ts",
    "generate": "pnpm generate:schemas && pnpm generate:exports"
  },
  "dependencies": {
    "@zodios/core": "^10.9.6",
    "papaparse": "^5.4.1",
    "yaml": "^2.6.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@redocly/cli": "^1.26.1",
    "@tsconfig/recommended": "^1.0.8",
    "@types/node": "~20.17.10",
    "@types/papaparse": "^5.3.15",
    "date-fns": "^4.1.0",
    "eslint": "^9.17.0",
    "husky": "^9.1.7",
    "lint-staged": "~15.3.0",
    "npm-check-updates": "^17.1.12",
    "openapi-zod-client": "^1.18.2",
    "renovate": "~39.106.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^2.1.8"
  },
  "lint-staged": {
    "*.{ts,json,yml,yaml}": [
      "prettier --write"
    ]
  }
}
